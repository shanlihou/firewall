{
  "name": "CatBlock",
  "version": "1.0",
  "description": "I can't has cheezburger!",
  "permissions": ["notifications",
				"webRequest", "webRequestBlocking",
			"*://*/*", "tabs"],
  "background": {
    "scripts": ["loldogs.js","util.js", "background.js"]
  },
  "browser_action": {  

	  "default_icon": "icon.png" ,

	  "default_title": "My Task List",

	  "default_popup": "popup.html"

  },

  "manifest_version": 2
}
